<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>	
  
  <comment>
    Key = unicode code point
    Entry value = replacement string
  </comment>
  
  <!-- Replace äöü by ae, oe and ue -->
  <entry key="00C4">Ae</entry>
  <entry key="00D6">Oe</entry>
  <entry key="00DC">Ue</entry>
  <entry key="00E4">ae</entry>
  <entry key="00F6">oe</entry>
  <entry key="00FC">ue</entry>
  
  <!-- Replace »,«,》,《 by " -->
  <entry key="00BB">"</entry>
  <entry key="300B">"</entry>
  <entry key="00AB">"</entry>
  <entry key="300A">"</entry>
  
</properties>
