<?xml version="1.0" encoding="utf-8"?>

<elementInfo>

    <abbrElement namespace="http://www.w3.org/1999/xhtml" prefix="" name="abbr" expandAttribute="title"/>

	<acronymElement namespace="http://www.w3.org/1999/xhtml" prefix="" name="acronym" expandAttribute="title">
		
	</acronymElement>
	
	<initialismElement namespace="http://www.w3.org/1999/xhtml" prefix="" name="acronym" expandAttribute="title">
		
